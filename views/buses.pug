extends layout
include buslist
include header
include filters

block content
  +header(backUrl, title)
  div(class='tableCount') #{totalCount} bus#{totalCount != 1 ? 'es' : ''}
  if filter == 'all' && showFilters
    +filters(thisUrl, filterTitle, staleTitle, oosTitle, showRetiredFilter)
  +buslist(buses, true, true)